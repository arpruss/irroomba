<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      android:versionCode="3"
      android:versionName="1.00" package="mobi.omegacentauri.irroomba">
    <!--  <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/> -->
    <application android:label="IR Roomba"
            android:icon="@drawable/icon"
    >
        <activity android:name=".IRRoomba"
            android:icon="@drawable/icon"
            android:screenOrientation="portrait"
            android:label="IR Roomba"
                              android:configChanges="keyboard|keyboardHidden|orientation"
                  android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Options"
            android:label="Settings"></activity>
    </application>
    <uses-sdk android:minSdkVersion="5" android:targetSdkVersion="13"/>
</manifest>
